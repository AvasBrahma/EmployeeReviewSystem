   <link rel="stylesheet" href="/css/viewallreviews.css">

         <div class="container bootstrap snippets bootdey tab-pane activity" id="activity-tab">
			<div class="row">
				<div class="col-lg-12">
					<div class="main-box no-header clearfix">
						<div class="main-box-body clearfix">
							<div class="table-responsive">
								<table class="table user-list">
									<thead>
										<tr>
										<th class="text-center"><span>Reviewer</span></th>
										<th class="text-center"><span>Reviewer Comment</span></th>
										<th class="text-center"><span>Review Created</span></th>
										<th class="text-center"><span>Performance Criteria</span></th>
										<th class="text-center"><span>Rating</span></th>
										<th class="text-center"><span>Employee Feedback Status</span></th>
                                        <th><span>Feedback Comment</span></th>
										<th>Actions</th>
										</tr>
									</thead>
									<tbody>
										<% reviews.forEach(element => { %>
										<tr>
											<td>
												<img src="https://bootdey.com/img/Content/user_1.jpg" alt="">
												<span><%= element.reviewername%></span>
											</td>
											<td>
												<span class="text-info"><%= element.feedback %></span>
											  </td>
											<td><%= formatCreatedAt(element.createdAt) %></td>
											<td>
												<span class="label label-info"><%= element.performancecriteria %></span>
											  </td>
											<td><a><%= element.rating %> Star</a></td>
											<td class="text-center">
												<% if (element.feedbackstatus === 'Submitted') { %>
                                                    <span class="label label-success"><%= element.feedbackstatus %></span>
													<% } else { %>
                                                        <span class="label label-danger"><%= element.feedbackstatus %></span>
												<% } %>
											</td>
											<td>
												<span class="text-info"><%= element.employeefeedback %></span>
											  </td>
											<td style="width: 20%;">
												<a href="#" class="table-link text-info">
													<span class="fa-stack">
														<i class="fa fa-square fa-stack-2x"></i>
														<i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
													</span>
												</a>
												<a href="#" class="table-link danger" onclick="openPopup()">
													<span class="fa-stack">
														<i class="fa fa-square fa-stack-2x"></i>
														<i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
													</span>
												</a>
											</td>
										</tr>
										<% }) %>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>